{
	"name": "@obsidize/tar-browserify",
	"version": "4.0.0",
	"description": "Browser-based tar utility for packing and unpacking tar files (stream-capable)",
	"main": "es5.js",
	"module": "index.js",
	"types": "index.d.ts",
	"typings": "index.d.ts",
	"scripts": {
		"test": "jest",
		"build:clean": "rimraf ./dist",
		"build:tsc": "tsc",
		"build:webpack": "webpack --config webpack.config.js",
		"copy:package": "cpy ./package.json ./dist/",
		"copy:readme": "cpy ./README.md ./dist/",
		"copy:assets": "run-p copy:package copy:readme",
		"build": "run-s build:clean build:tsc build:webpack copy:assets",
		"coverage:clean": "rimraf ./coverage",
		"coverage:open": "open-cli ./coverage/lcov-report/index.html",
		"coverage": "run-s coverage:clean test coverage:open",
		"docs": "typedoc ./src/index.ts --out ./docs --excludePrivate",
		"docs:open": "open-cli ./docs/index.html",
		"git:publish": "node ./scripts/release-publish.js",
		"dist:publish": "npm publish ./dist",
		"dist:pack": "node ./scripts/release-pack.js",
		"dist:bundle": "run-s build coverage docs dist:pack",
		"version": "npm run dist:bundle",
		"version:patch": "npm version patch",
		"version:publish": "run-s git:publish dist:publish",
		"bump": "run-s version:patch version:publish",
		"generate:tar:assets": "node ./scripts/generate-tarball-test-assets.js",
		"generate:tar:json:postbuild": "node ./scripts/generate-tar-example-json.js",
		"generate:tar:json": "run-s build generate:tar:json:postbuild",
		"generate:tar:output:postbuild": "node ./scripts/generate-tar-example-file.js",
		"generate:tar:output": "run-s build generate:tar:output:postbuild"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jospete/obsidize-tar-browserify.git"
	},
	"keywords": [
		"tar",
		"browser",
		"tarball",
		"browserify",
		"ustar",
		"gnutar",
		"obsidize"
	],
	"author": "Josh Noel <obsidize@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jospete/obsidize-tar-browserify/issues"
	},
	"homepage": "https://github.com/jospete/obsidize-tar-browserify#readme",
	"dependencies": {
		"tslib": "2.4.1"
	},
	"devDependencies": {
		"@babel/core": "7.26.0",
		"@babel/preset-env": "7.26.0",
		"@babel/preset-typescript": "7.26.0",
		"@types/fs-extra": "11.0.1",
		"@types/jest": "29.5.14",
		"@types/yargs": "17.0.20",
		"@webpack-cli/generators": "^3.0.7",
		"babel-jest": "29.7.0",
		"cpy-cli": "4.2.0",
		"fs-extra": "11.1.0",
		"globby": "13.1.3",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jsdom": "25.0.1",
		"npm-run-all": "4.1.5",
		"open-cli": "7.1.0",
		"rimraf": "4.1.1",
		"tar": "6.1.13",
		"ts-loader": "9.4.2",
		"tsx": "4.19.2",
		"typedoc": "0.23.24",
		"typescript": "4.9.4",
		"webpack": "5.75.0",
		"webpack-cli": "5.0.1",
		"yargs": "17.6.2"
	}
}
