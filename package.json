{
  "name": "@obsidize/tar-browserify",
  "version": "2.1.2",
  "description": "Browser-based tar utility for packing and unpacking small tar files",
  "main": "es5.js",
  "module": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "scripts": {
    "test": "karma start",
    "test:single": "karma start --single-run",
    "build:webpack": "webpack --config webpack.config.js",
    "build:tsc": "tsc",
    "build": "run-s build:tsc build:webpack copy:assets",
    "clean": "rimraf ./dist ./coverage",
    "rebuild": "run-s clean build",
    "coverage:open": "open-cli \".\\coverage\\*\\html\\index.html\"",
    "coverage": "run-s test:single coverage:open",
    "copy:package": "cpy ./package.json ./dist/",
    "copy:readme": "cpy ./README.md ./dist/",
    "copy:assets": "run-p copy:package copy:readme",
    "git:publish": "node ./scripts/release-publish.js",
    "dist:publish": "npm publish dist",
    "release:prepare": "node ./scripts/release-prepare.js",
    "release:test": "run-s rebuild coverage docs",
    "release:publish": "run-s git:publish dist:publish",
    "release:prepare:patch": "npm run release:prepare -- --release-as patch",
    "bump": "run-s release:prepare:patch release:test release:publish",
    "docs": "typedoc ./src/index.ts --out ./docs --excludePrivate",
    "docs:open": "open-cli \".\\docs\\index.html\"",
    "generate:tar:assets": "ts-node ./scripts/generate-tarball-test-assets.js",
    "generate:tar:json": "npm run rebuild && node ./scripts/generate-tar-example-json.js",
    "generate:tar:output": "npm run rebuild && node ./scripts/generate-tar-example-file.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jospete/obsidize-tar-browserify.git"
  },
  "keywords": [
    "obsidize",
    "tar",
    "tarball",
    "browser",
    "browserify"
  ],
  "author": "Josh Noel <obsidize@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jospete/obsidize-tar-browserify/issues"
  },
  "homepage": "https://github.com/jospete/obsidize-tar-browserify#readme",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.12",
    "@types/jasmine": "^3.6.3",
    "cpy-cli": "^3.1.1",
    "fs-extra": "10.0.0",
    "globby": "12.0.2",
    "jasmine-core": "^3.6.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-typescript": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "6.6.7",
    "tar": "6.1.11",
    "ts-loader": "^8.0.18",
    "ts-node": "10.2.1",
    "typedoc": "0.21.4",
    "typescript": "4.1.5",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "yargs": "^16.2.0"
  }
}
